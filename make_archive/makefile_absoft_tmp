# Tunable parameters
#
# CF		Name of the fortran compiling system to use
# LDFLAGS	Flags to the loader
# LIBS		List of libraries
# CMD		Name of the executable
# PROFLIB	Library needed for profiling
#
CC =		/bin/cc
CF =		f77
#For f90:
#LDFLAGS = -G1 -Wl"-i dirfile"
#LDFLAGS =	-g  -Wl"-i dirfile -M map.out"
#LDFLAGS =	-g
#LDFLAGS2 =	-g -Wl"-i dirfile -f indef"
#LDFLAGS2 =	-g  -Wl"-i dirfile -M map.out"
#LIBS =		-Wl"$(GRAFLIB)" -Wl"$(NAG15)" -Wl"$(IMSLMATH)" -Wl"$(IMSLSTAT)"\
#	        -Wl"$(LIBMATH)" -Wl"$(IMSLSFUN)"
#  LIBS =          -Wl"$(GRAFLIB)" -Wl"$(NAG15)" -Wl"/U1/u1435/libraries/imsl.a"\
#                  -Wl"$(LIBMATH)"\
#                  -Wl"$(IMSLMATH)" -Wl"$(IMSLSTAT)" -Wl"$(IMSLSFUN)"
#LIBS =          -Wl"$(GRAFLIB)"\
#                   -Wl"/U1/u1435/libraries/imsl.a" -Wl"/usr/local/lib/libmath.a"
CMD =		cqlp
#PROFLIB =	-lprof

# To perform the default compilation, use the first line
# To compile with flowtracing turned on, use the second line
# To compile giving profile additonal information, use the third line
# WARNING:  SIMULTANEOUSLY PROFILING AND FLOWTRACING IS NOT RECOMMENDED 
# for listing: FFLAGS =	 -Wf" -e mcxn"
#FFLAGS =	 -F
##FFLAGS =	 -Wf" -ez "
# For f90:
#FFLAGS = -G1 -ev

# For Absoft:
# -s    static storage
# -f    forces all symbols to lower case
# -N15  appends and underscore to all external names in Fortran programs.
# -N34  automatically align common block variables.
# -X    directs message to Linux loader ld.
# -noinhibit-exec  gives executable, even when there are unsatisfied refs.
# -M    give map to standard o/p.
FFLAGS = -s -f  -g -N15 -N34 -v
LDFLAGS =	-g  -X-noinhibit-exec -X-M 
LIBS =  -limsl  -llapack -lblas -lnetcdf  -L/usr/local/pgplot -lpgplot -L/usr/lib  -lU77 -lf2c -L/usr/X11R6/lib -L/usr/X11/lib -lX11 -lm -L/usr/local/pgplot -lpgplot -L/usr/local/lib -lnetcdf

#.F.f:
#	$(CF77PP) $(CF77PPFLAGS) $< $*.f
.F.o:
#	$(CF77PP) $(CF77PPFLAGS) $< $*.f
	$(CF) $(FFLAGS) -c $*.f

# Lines from here on down should not need to be changed.  They are the
# actual rules which make uses to build cqlp.
#
all:	get incOK $(CMD) $(CMD).prof put
#all:	get  $(CMD) $(CMD).prof put

$(CMD):		$(INCS) $(OBJS) 
	$(CF) $(LDFLAGS) -o $(@) $(OBJS) $(LIBS)

segldr:	
	$(CF) $(LDFLAGS2) -o $(CMD) $(OBJS) $(LIBS)

# Make the profiled version of the command and call it cqlp.prof
#
$(CMD).prof:	$(OBJS)
	$(CF) $(LDFLAGS) -o $(@) $(OBJS) $(PROFLIB) $(LIBS)
incOK:	$(INCS)
	touch incOK  $(SRCS)
get:	
#	cp $(MKDIR)/sub/*.F .
	cp $(MKDIR)/sub/*.f .
	cp $(MKDIR)/sub/*.c .
	cp $(MKDIR)/obj/*.o .
	cp $(MKDIR)/inc/*.h .
put:
	cp $(SRCS) *.f  $(MKDIR)/sub/
	cp $(SRCS)  $(MKDIR)/sub/
	cp *.o $(MKDIR)/obj/
	cp *.h $(MKDIR)/inc/
tar_so:
#	cp $(MKDIR)/sub/*.F .
#	cp $(MKDIR)/sub/*.f .
#	cp $(MKDIR)/sub/*.c .
#	cp $(MKDIR)/inc/*.h .
#	cp $(MKDIR)/inp/c* .
#	cp $(MKDIR)/inp/ray* .
#	cp $(MKDIR)/inp/eqdsk* .
#	cp $(MKDIR)/obj/*.o .
	tar cf so$(CFSVERS)tar *.f *.F *.c *.h  make* cqli* ray* eqd* dir* *.o o.test* cbtes* runt*
	cfs store cqlp/so$(CFSVERS)tar
	mv so$(CFSVERS)tar ../so$(CFSVERS)tar
tar_s:
#	cp $(MKDIR)/sub/*.F .
#	cp $(MKDIR)/sub/*.f .
#	cp $(MKDIR)/sub/*.c .
#	cp $(MKDIR)/inc/*.h .
#	cp $(MKDIR)/inp/cqli* .
#	cp $(MKDIR)/inp/ray* .
#	cp $(MKDIR)/inp/eqdsk* .
##	tar cf ../$(CFSVERS).tar *.f *.c      *.h  make* cqli* ray* eqd*
	tar cf $(CFSVERS).tar *.f *.F *.c *.h  make* cqli* ray* eqd* dir* o.test* cbtes* runt*
	cfs store cqlp/$(CFSVERS).tar
	mv $(CFSVERS).tar ../$(CFSVERS).tar
tar_sr:
#	cp $(MKDIR)/sub/*.F .
#	cp $(MKDIR)/sub/*.f .
#	cp $(MKDIR)/sub/*.c .
#	cp $(MKDIR)/inc/*.h .
#	cp $(MKDIR)/inp/c* .
#	cp $(MKDIR)/inp/ray* .
#	cp $(MKDIR)/inp/eqdsk* .
#	tar cf s_$(CFSVERS).tar *.F *.c *.f  *.h  make* c* ray* eqd*
#	cfs replace cqlp/s_$(CFSVERS).tar
touch:
	touch -a *
	touch -a $(MKDIR)/sub/*
	touch -a $(MKDIR)/obj/*
	touch -a $(MKDIR)/inc/*
clean:
	-rm -f $($(MKDIR)/objS)

clobber:	clean
	-rm -f $(CMD) $(CMD).prof

void:	clobber
	-rm -f $(SRCS) makefile

init:	
	for x in *.F; do \
	cp $$x `basename $$x .F`.f; \
	done

unsplit:
##	cat $(SRCS) > ../$(CMD).f
	cp $(INCS) ..
	cat $(INCS) > ../$(VERSION).f
	cat $(SRCS) >> ../$(VERSION).f

listing:
	/bin/cc -opagenumberlist.exe pagenumberlist.c
	echo " " > sou.update
	rm sou.update
	for x in $(INCS); do \
	  echo "*COMDECK "$$x >> sou.update; \
	  cat $$x >> sou.update; \
	done
	for x in $(SRCSUP); do \
	  echo "*DECK "$$x  >> sou.update; \
	  cat $$x >> sou.update; \
	done
	time pagenumberlist.exe < sou.update >../$(VERSION).l
	rm sou.update
